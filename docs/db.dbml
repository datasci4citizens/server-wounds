virtual VirtualPatient:
    patient_id            <-- person_row.person_id
    birthday              <-- person_row.birth_datetime
    gender                <-- person_row.gender_concept_id
    specialist_id         <-- person_row.provider_id
    hospital_registration <-- person_row.care_site_id
    height                <-- height_row.value_as_number
    updated_at            <-- height_row.measurement_date
    weight                <-- weight_row.value_as_number
    smoke_frequency       <-- smoke_frequency.value_as_concept_id
    drink_frequency       <-- drink_frequency.value_as_concept_id
    bind person_row:
        * person_id            <-> patient_id
          birth_datetime       <-> birthday
          year_of_birth        === 0
          race_concept_id      === 0
          ethnicity_concept_id === 0
          gender_concept_id    <-> gender
          provider_id          <-> specialist_id
          care_site_id         <-> hospital_registration

    bind height_row:
        * person_id                   <-- patient_id
          value_as_number             <-> height
        * measurement_concept_id      === 607590
          unit_concept_id             === 8582
          measurement_date            <-> updated_at
          measurement_type_concept_id === 0

    bind weight_row:
        * person_id                   <-- patient_id
          value_as_number             <-> weight
        * measurement_concept_id      === 4099154
          unit_concept_id             === 9529
          measurement_date            <-- updated_at
          measurement_type_concept_id === 0

    bind smoke_frequency:
        * person_id                   <-- patient_id
        * observation_concept_id      === 1586177
          value_as_concept_id         <-> smoke_frequency
          observation_date            <-- updated_at
          observation_type_concept_id === 0

    bind drink_frequency:
        * person_id                   <-- patient_id
        * observation_concept_id      === 40771103
          value_as_concept_id         <-> drink_frequency
          observation_date            <-- updated_at
          observation_type_concept_id === 0


